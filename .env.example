# Сервер
PORT=3000
NODE_ENV=production

# База данных (обязательно!)
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/serverreport

# JWT (обязательно - должен быть уникальным!)
# Сгенерируйте свой ключ: openssl rand -hex 32
JWT_SECRET=ЗАМЕНИТЕ_НА_СВОЙ_СЕКРЕТНЫЙ_КЛЮЧ_МИНИМУМ_32_СИМВОЛА
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=*
